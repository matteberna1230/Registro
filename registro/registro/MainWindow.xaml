        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:registro"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtNome" HorizontalAlignment="Left" Margin="89,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="68"/>
        <TextBox x:Name="txtCognome" HorizontalAlignment="Left" Height="68" Margin="389,97,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155"/>
        <Button x:Name="btnInserisci" Content="Inserisci" HorizontalAlignment="Left" Margin="657,97,0,0" VerticalAlignment="Top" Height="68" Width="87" Click="btnInserisci_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="89,76,0,0" TextWrapping="Wrap" Text="Nome:" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Height="16" Margin="389,76,0,0" TextWrapping="Wrap" Text="Cognome:" VerticalAlignment="Top" Width="52"/>

    </Grid>
</Window>
